{
    "opts": {
        "template": "node_modules/latodoc",
        "destination": "./jsdoc/",
        "recurse": true//,
        //"tutorials": "tutorials"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
          "./"
        ],
        "exclude": [
          "node_modules",
          "jsdoc"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
